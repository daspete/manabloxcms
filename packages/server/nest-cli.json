{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/manablox/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/manablox/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/manablox",
  "projects": {
    "manablox": {
      "type": "application",
      "root": "apps/manablox",
      "entryFile": "main",
      "sourceRoot": "apps/manablox/src",
      "compilerOptions": {
        "tsConfigPath": "apps/manablox/tsconfig.app.json"
      }
    },
    "manablox-files": {
      "type": "application",
      "root": "apps/manablox-files",
      "entryFile": "main",
      "sourceRoot": "apps/manablox-files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/manablox-files/tsconfig.app.json"
      }
    }
  }
}
